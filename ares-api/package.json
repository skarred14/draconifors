{
  "name": "ares",
  "version": "1.0.0",
  "description": "Ares Microservice",
  "main": "src/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon ./dist/index.js",
    "build": "cod-scripts build",
    "circuits": "onchange -a '**/*.circom' -- node './dist/compile.js' {{changed}}",
    "build:watch": "npm run build -- --watch & npm run circuits",
    "test": "jest --watchAll --verbose=true --silent",
    "test:ci": "jest --verbose=true --silent --ci --coverage",
    "fix": "npm run format && npm run lint -- --fix",
    "format": "prettier --write \"**/*.{json,css,scss,md}\"",
    "lint": "eslint .",
    "lint:md": "markdownlint -i node_modules -i dist .",
    "validate": "npm run lint && npm run lint:md"
  },
  "author": "",
  "license": "CC0",
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "body-parser": "1.19.0",
    "circom": "^0.0.35",
    "config": "^3.2.4",
    "cors": "2.8.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "^4.17.1",
    "mongodb": "^3.3.2",
    "snarkjs": "^0.1.20",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "cod-scripts": "^3.2.0",
    "markdownlint-cli": "^0.15.0",
    "jest": "^24.9.0",
    "nodemon": "^1.18.9",
    "onchange": "^6.1.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/cod-scripts/eslint.js"
    ]
  }
}
